{
  "name": "vack",
  "version": "1.1.3",
  "description": "a cli based on webapck for react and vue",
  "bin": "/bin/vack.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf ./dist && npx tsc && npm run prettier && npm run lint",
    "lint": "npx eslint bin config dist src template --fix",
    "prettier": "npx prettier --write bin config dist src template"
  },
  "keywords": [
    "webpack",
    "cli",
    "react",
    "vue",
    "vack"
  ],
  "author": "bytejitter",
  "repository": "https://github.com/ByteJitter/vack.git",
  "license": "MIT",
  "dependencies": {
    "@types/cross-spawn": "^6.0.2",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "cross-spawn": "^7.0.3",
    "inquirer": "^8.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-typescript": "^7.14.5",
    "@types/inquirer": "^7.3.2",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "2.3.1",
    "typescript": "^4.3.4"
  }
}
